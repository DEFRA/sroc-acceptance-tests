{
  "name": "sroc-acceptance-tests",
  "version": "1.0.0",
  "description": "Automated acceptance tests for the SROC Tactical Charging Module",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:open:local": "cypress open --env environment=local",
    "cy:open:dev": "cypress open --env environment=dev",
    "cy:open:tst": "cypress open --env environment=tst",
    "cy:open:pre": "cypress open --env environment=pre",
    "cy:run": "cypress run",
    "cy:run:local": "cypress run --env environment=local",
    "cy:run:dev": "cypress run --env environment=dev",
    "cy:run:tst": "cypress run --env environment=tst",
    "cy:run:pre": "cypress run --env environment=pre",
    "lint": "standard",
    "test": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEFRA/sroc-acceptance-tests.git"
  },
  "author": "Department for Environment, Food & Rural Affairs",
  "license": "OGL-UK-3.0",
  "bugs": {
    "url": "https://github.com/DEFRA/sroc-acceptance-tests/issues"
  },
  "homepage": "https://github.com/DEFRA/sroc-acceptance-tests#readme",
  "dependencies": {
    "cypress": "^8.3.1",
    "cypress-cucumber-preprocessor": "^4.2.0",
    "cypress-dotenv": "^1.2.2",
    "dotenv": "^8.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "standard": "^16.0.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "standard": {
    "globals": [
      "cy",
      "Cypress",
      "expect"
    ]
  }
}
